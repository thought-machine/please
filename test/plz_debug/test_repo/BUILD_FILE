# Dummy build rule used to show that both `debug_cmd` and `debug_tools` work as expected.
build_rule(
    name = "hello_world",
    cmd = {
        'dbg': "echo 'hello world' > $OUT",
    },
    outs = ["out"],
    debug_cmd = "$TOOLS_CAT ./$OUT",
    debug_tools = {
        'cat': 'cat'
    },
    binary = True,
)
